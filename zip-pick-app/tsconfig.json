{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "App.tsx",
    "app/**/*",
    "components/**/*"
  ],
  "exclude": [
    "node_modules",
    "app/app.tsx"
  ]
}
